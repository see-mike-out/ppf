<div class="container">
  <div class="row">
    <div class="l3 m12 s12">
      <div class="content-wrap">
        {{#if is_en}}
          <h1>Tables</h1>
        {{else}}
          <h1>표</h1>
        {{/if}}
      </div>
    </div>
    <div class="l9 m12 s12">
      <div class="content-wrap">
        <div class="sublist_foldable" data-open="false">
          <div class="folder" id="table_list">
            {{#if is_en}}
              <span>List of Tables</span>
            {{else}}
              <span>표 목록</span>
            {{/if}}
          </div>
        </div>
      </div>
    </div>
  </div>
  
  <div class="row">
    
    <div class="l12 m12 s12">
      <div class="content-wrap">
        <div id="table_area"></div>
      </div>
    </div>
    
  </div>
</div>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/d3-format/1.3.0/d3-format.js"></script>
<script src="/wpmy/js/wpmy_utility.js"></script>
<script src="/wpmy/js/table_control.js"></script>
<script>
  create_table_list('#table_list', '#table_area', "{{ln}}");
  get_table('#table_area', "gini", "{{ln}}");
  $('.sublist_foldable')
    .on('mouseover', function() { $(this).attr('data-open', 'true') })
    .on('mouseout', function() { $(this).attr('data-open', 'false') });
</script>